<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="HeadingDashboard">
        <item name="android:fontFamily">@font/ttnorms_bold</item>
        <item name="android:textSize">34sp</item>
        <item name="android:textColor">@color/grey_dark</item>
    </style>

    <style name="SubHeadingGeneral">
        <item name="android:fontFamily">@font/ttnorms_bold</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#979EA6</item>
    </style>

    <style name="SubHeadingDashboard">
        <item name="android:fontFamily">@font/gilroy_bold</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/grey_font_light</item>
    </style>

    <style name="SubSubHeadingDashboard">
        <item name="android:fontFamily">@font/gilroy_bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/grey_font_light</item>
    </style>

    <style name="paragraphDashboard">
        <item name="android:fontFamily">@font/actor_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/grey_font_light</item>
    </style>

    <style name="descriptionKey">
        <item name="android:fontFamily">@font/europa_regular_webfont</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#BABEC1</item>
    </style>

    <style name="descriptionValue">
        <item name="android:fontFamily">@font/europa_regular_webfont</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#272727</item>
        <item name="android:textStyle">bold</item>
    </style>
</resources>